{
    "name": "rollerworks/multi-user-bundle",
    "type": "symfony-bundle",
    "description": "Multi user management for the FOSUserBundle",
    "keywords": ["rollerworks", "multi", "user", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php":                          ">=5.3.3",
        "symfony/framework-bundle":     "~2.2",
        "symfony/finder":               "~2.2",
        "friendsofsymfony/user-bundle": "~2.0@dev"
    },
    "require-dev": {
        "twig/twig":                "~1.5",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm":             "~2.2,>=2.2.3",
        "swiftmailer/swiftmailer":  ">=4.3,<6.0",

        "symfony/yaml":                "~2.2",
        "symfony/validator":           "~2.2",
        "symfony/twig-bundle":         "~2.2",
        "symfony/monolog-bundle":      "~2.2",
        "symfony/swiftmailer-bundle":  "~2.2",
        "symfony/web-profiler-bundle": "~2.2",
        "symfony/browser-kit":         "~2.2",
        "symfony/css-selector":        "~2.2",
        "symfony/process":             "~2.2"
    },
    "conflicts": [
        "pugx/multi-user-bundle"
    ],
    "autoload": {
        "psr-0": {
            "Rollerworks\\Bundle\\MultiUserBundle": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
